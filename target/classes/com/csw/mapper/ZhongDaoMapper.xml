<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.csw.dao.ZhongDao">
    <delete id="delete">
    delete from zhong where pid=#{delete}
</delete>
    <insert id="add">
        insert into zhong values(#{z.pid},#{z.cid})
    </insert>
</mapper>
